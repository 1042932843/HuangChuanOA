<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:overScrollMode="never"
    >

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/colorF">

    <TextView
        android:id="@+id/cy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="常用应用"
        android:textColor="#000"
        android:textSize="18dp" />

    <LinearLayout
        android:id="@+id/lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cy"
        android:background="@color/colorF"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x35"
            android:layout_marginRight="@dimen/x35"
            android:layout_marginTop="@dimen/y50"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0">

                <TextView
                    android:id="@+id/kaoqin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/setting_item_selector"
                    android:drawablePadding="8dp"
                    android:drawableTop="@mipmap/kq"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="签到"
                    android:textColor="#888888"
                    android:textSize="11dp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0">

                <TextView
                    android:id="@+id/calendar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/setting_item_selector"
                    android:drawablePadding="8dp"
                    android:drawableTop="@mipmap/rc"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="日程"
                    android:textColor="#888888"
                    android:textSize="11dp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0">

                <TextView
                    android:id="@+id/email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/setting_item_selector"
                    android:drawablePadding="8dp"
                    android:drawableTop="@mipmap/rj"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="邮件"
                    android:textColor="#888888"
                    android:textSize="11dp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0">

                <TextView
                    android:id="@+id/meeting"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/setting_item_selector"
                    android:drawablePadding="8dp"
                    android:drawableTop="@mipmap/hy"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="会议"
                    android:textColor="#888888"
                    android:textSize="11dp" />
            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="4dp"
            android:layout_marginLeft="@dimen/x35"
            android:layout_marginRight="@dimen/x35"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0">


                <TextView
                    android:id="@+id/map"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/setting_item_selector"
                    android:drawablePadding="8dp"
                    android:drawableTop="@mipmap/qd"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="考勤"
                    android:textColor="#888888"
                    android:textSize="11dp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0">


                <TextView
                    android:id="@+id/toDo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/setting_item_selector"
                    android:drawablePadding="8dp"
                    android:drawableTop="@mipmap/dbrw"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="待办任务"
                    android:textColor="#888888"
                    android:textSize="11dp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0">


                <TextView
                    android:id="@+id/new_bpm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/setting_item_selector"
                    android:drawablePadding="8dp"
                    android:drawableTop="@mipmap/xjlc"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="新建流程"
                    android:textColor="#888888"
                    android:textSize="11dp" />
            </RelativeLayout>

            <!--
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0"
                            >


                        <TextView
                            android:id="@+id/my_bpm"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/setting_item_selector"
                            android:drawablePadding="8dp"
                            android:drawableTop="@mipmap/wdlc"
                            android:gravity="center"
                            android:padding="10dp"
                            android:text="我的流程"
                            android:textColor="#888888"
                            android:textSize="11dp"
                            android:visibility="gone" />
                        </RelativeLayout>
            -->

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0">


                <TextView
                    android:id="@+id/my_kqjl"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/setting_item_selector"
                    android:drawablePadding="8dp"
                    android:drawableTop="@mipmap/kqjl"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="考勤图表"
                    android:textColor="#888888"
                    android:textSize="11dp"
                    android:visibility="visible" />
            </RelativeLayout>


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="4dp"
            android:layout_marginLeft="@dimen/x35"
            android:layout_marginRight="@dimen/x35"
            android:orientation="horizontal"
            android:visibility="visible">

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0"
                android:visibility="visible">

                <TextView
                    android:id="@+id/wdsq"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/setting_item_selector"
                    android:drawablePadding="8dp"
                    android:drawableTop="@mipmap/wdsq"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="我的申请"
                    android:textColor="#888888"
                    android:textSize="11dp" />
            </RelativeLayout>

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0">


                <TextView
                    android:id="@+id/wdcg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/setting_item_selector"
                    android:drawablePadding="8dp"
                    android:drawableTop="@mipmap/wdcg"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="我的草稿"
                    android:textColor="#888888"
                    android:textSize="11dp"
                    android:visibility="gone" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0">


            </RelativeLayout>

            <!--
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0"
                            >


                        <TextView
                            android:id="@+id/my_bpm"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/setting_item_selector"
                            android:drawablePadding="8dp"
                            android:drawableTop="@mipmap/wdlc"
                            android:gravity="center"
                            android:padding="10dp"
                            android:text="我的流程"
                            android:textColor="#888888"
                            android:textSize="11dp"
                            android:visibility="gone" />
                        </RelativeLayout>
            -->

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0">


            </RelativeLayout>


        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/lin1"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_below="@+id/lin"
        android:background="#EEEEEE"
        android:orientation="horizontal" />

    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lin1"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="新闻公告"
        android:textColor="#000"
        android:textSize="18dp" />

    <LinearLayout
        android:id="@+id/lin2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text1"
        android:layout_marginBottom="4dp"
        android:layout_marginLeft="@dimen/x35"
        android:layout_marginRight="@dimen/x35"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.0">

            <TextView
                android:id="@+id/gzgs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/setting_item_selector"
                android:drawablePadding="8dp"
                android:drawableTop="@mipmap/gzgs"
                android:gravity="center"
                android:padding="10dp"
                android:text="公告公示"
                android:textColor="#888888"
                android:textSize="11dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.0">

            <TextView
                android:id="@+id/dzgw"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/setting_item_selector"
                android:drawablePadding="8dp"
                android:drawableTop="@mipmap/dzgw"
                android:gravity="center"
                android:padding="10dp"
                android:text="电子公文"
                android:textColor="#888888"
                android:textSize="11dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.0">

            <TextView
                android:id="@+id/gzzd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/setting_item_selector"
                android:drawablePadding="8dp"
                android:drawableTop="@mipmap/gzzd"
                android:gravity="center"
                android:padding="10dp"
                android:text="规章制度"
                android:textColor="#888888"
                android:textSize="11dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.0">

            <TextView
                android:id="@+id/zcfg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/setting_item_selector"
                android:drawablePadding="8dp"
                android:drawableTop="@mipmap/zcfg"
                android:gravity="center"
                android:padding="10dp"
                android:text="政策法规"
                android:textColor="#888888"
                android:textSize="11dp" />
        </RelativeLayout>

    </LinearLayout>
    <!--
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0dp"
                android:layout_alignParentBottom="true"
                android:background="@color/black5" />
        </RelativeLayout>
    -->


</RelativeLayout>
    </ScrollView>